{
	"name": "laptrinhio/lara4strap",
	"description": "The basic bootstrap template for tutorials in laptrinh.io",
	"keywords": ["bootstrap", "laravel", "template", "lập trình"],
	"license": "MIT",
	"type": "project",
    "repositories": [ {
        "type": "package",
        "package": {
            "name": "laptrinhio/lara4strap",
            "version": "0.1.0",
            "source": {
                "url": "https://github.com/laptrinhio/lara4strap.git",
                "type": "git",
                "reference": "master"
              }
         }
    }],
	"require": {
		"laravel/framework": "4.2.*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
